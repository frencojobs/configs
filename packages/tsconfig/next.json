{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "incremental": true,
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true
  }
}
